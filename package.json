{
    "name": "@monorepo-devjobs/root",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "client/*",
        "server/*",
        "packages/*"
    ],
    "scripts": {
        "start": "node server/src/index.ts",
        "server": "nodemon server/src/index.ts",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\" "
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.1"
    },
    "dependencies": {
        "classnames": "^2.3.2"
    }
}
